    "@biomejs/biome": "1.7.2",
    "react": "18.3.1",
    "react-dom": "18.3.1",
    "rollup": "4.17.2",
    "standard-version": "9.5.0",
    "tslib": "2.6.2",
    "tsup": "8.0.2",
