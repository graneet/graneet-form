  "devDependencies": {
    "@types/node": "22.13.5",
    "@types/react": "19.0.10",
    "react": "19.0.0",
    "react-dom": "19.0.0",
    "rollup": "4.34.8",
    "tslib": "2.8.1",
    "tsup": "8.3.6",
    "typescript": "5.7.3"
  },
  "peerDependencies": {
    "react": "^16.8.0 || ^17 || ^18 || ^19"
